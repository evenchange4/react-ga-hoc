{
  "name": "react-ga-hoc",
  "version": "3.0.0",
  "description": "Send GA pageview safely with async data.",
  "author": "Michael Hsu",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/evenchange4/react-ga-hoc/issues"
  },
  "homepage": "https://github.com/evenchange4/react-ga-hoc#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/evenchange4/react-ga-hoc.git"
  },
  "main": "lib/index.js",
  "files": [
    "lib",
    "src",
    "test"
  ],
  "keywords": [
    "react",
    "ga",
    "react-ga",
    "HOC",
    "decorator"
  ],
  "scripts": {
    "clean": "rm -rf lib",
    "prebuild": "npm run clean",
    "build": "NODE_ENV=production babel src --out-dir lib",
    "test": "ava",
    "test:watch": "npm test -- --watch",
    "test:coverage": "nyc npm run test",
    "coverage": "nyc report --reporter=lcov && codecov",
    "lint:js": "eslint --ignore-path .gitignore .",
    "lint": "npm run lint:js",
    "prepublish": "npm run build"
  },
  "peerDependencies": {
    "react": ">= 0.14.0",
    "react-ga": "^2.1.2"
  },
  "dependencies": {
    "react-lifecycle-hoc": "^3.1.0",
    "recompose": "^0.22.0"
  },
  "devDependencies": {
    "ava": "^0.17.0",
    "babel-cli": "^6.18.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-register": "^6.18.0",
    "codecov": "^1.0.1",
    "enzyme": "^2.5.1",
    "eslint": "^3.9.1",
    "jsdom": "^9.8.3",
    "lint-config-m": "^3.0.0",
    "nyc": "^10.0.0",
    "react": "^15.3.2",
    "react-addons-test-utils": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-ga": "^2.1.2",
    "stylelint": "^7.5.0"
  },
  "babel": {
    "presets": [
      "react",
      "es2015",
      "stage-0"
    ],
    "plugins": [
      "add-module-exports"
    ]
  },
  "ava": {
    "files": [
      "test/**/*.test.js"
    ],
    "failFast": true,
    "verbose": true,
    "require": [
      "babel-register",
      "./test/.setup-browser-env.js"
    ],
    "babel": "inherit"
  },
  "nyc": {
    "exclude": [
      "test/**"
    ],
    "include": [
      "src/**"
    ]
  },
  "eslintConfig": {
    "extends": [
      "./node_modules/lint-config-m/eslint.js"
    ]
  }
}
